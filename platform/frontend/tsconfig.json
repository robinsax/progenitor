{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "outDir": "dist",
        "declaration": false,
        "moduleResolution": "node",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
